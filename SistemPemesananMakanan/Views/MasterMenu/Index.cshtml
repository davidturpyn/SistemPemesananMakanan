@model ICollection<MasterMenu>
@{
    ViewData["Title"] = "Master Menu";
}
<h1>@ViewData["Title"]</h1>


@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nama</th>
                <th scope="col">Jenis</th>
                <th scope="col">Harga</th>
                <th scope="col">Status</th>
                @* <th scope="col">Action</th> *@
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th>@item.Nama</th>
                    <td>@item.Jenis</td>
                    <td>@item.Harga</td>
                    <td>@item.Status</td>
                    @* <td>
                        <a class="btn btn-warning" asp-action="Edit" asp-controller="MasterMenu" asp-route-id="@item.Id">Edit</a>
                        <a class="btn btn-danger" asp-action="Delete" asp-controller="MasterMenu" asp-route-id="@item.Id">Delete</a>
                    </td> *@
                </tr>
            }
        </tbody>
    </table>
}
